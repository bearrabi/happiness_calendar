<!DOCTYPE html>
<html>
  <head>
    <title>Happinesscalendar</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render partial: "parts/headbar" %>
    <div id="main">
      <%= render partial: "parts/leftbar" %>
      <%= yield %>
      
    </div>
  </body>
</html>
